{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/paymentsuccess",
        "headers": { "Location": "https://q-shop-hosted-frontend.vercel.app" }
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }